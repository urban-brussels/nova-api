{
    "name": "urban-brussels/nova-api",
    "description": "PHP wrapper for Nova API. Nova is a shared IT platform of the Brussels-Capital Region dedicated to the file management of planning permits, land division permits and environmental licences.",
    "homepage": "https://github.com/urban-brussels/nova-api",
    "keywords": ["Brussels","Planning permits","Environmental licences"],
    "require": {
        "php": "^8.1",
        "symfony/http-client": "6.*",
        "ici-be/ici-tools": "0.3.4",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "maennchen/zipstream-php": "^2.1",
        "ext-intl": "*",
        "wgirhad/geophp": "^3.0"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "UrbanBrussels\\NovaApi\\": "src/"
        }
    }
}
